   "source": [
    "bins = [0,9.9,14.9,19.9,24.9,29.9,34.9,39.9,1000000000]\n",
    "column_number = [\"<10\",\"10-14\",\"15-19\",\"20-24\",\"25-29\",\"30-34\",\"35-39\",\"40+\"]\n",
    "\n",
    "duplicate_drop[\"range\"] = pd.cut(duplicate_drop[\"Age\"],bins,labels=column_number)\n",
    "\n",
    "total_count = duplicate_drop[\"range\"].value_counts()\n",
    "total_count\n",
    "\n",
    "percentage_players = total_count/player_count\n",
    "percentage_players\n",
    "\n",
    "#Summary Data Frame\n",
    "age_demographics_df = pd.DataFrame({\"Total Count\": total_count,\n",
    "                                    \"Percentage of Player\": percentage_players})\n",
    "age_demographics_df.head()\n",
    "\n",
    "# Sort the DataFrame by index\n",
    "sort_index = age_demographics_df.sort_index()\n",
    "sort_index.head()"
   ]
