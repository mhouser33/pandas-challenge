   "source": [
    "#Purchase Count\n",
    "purchase_counts = purchase_data_df.groupby([\"Gender\"]).count()[\"Price\"]\n",
    "purchase_counts\n",
    "\n",
    "#Total Purchase Value\n",
    "gender_purchase_total = purchase_data_df.groupby([\"Gender\"]).sum()[\"Price\"]\n",
    "gender_purchase_total\n",
    "\n",
    "#Average Purchase Price\n",
    "gender_average = purchase_data_df.groupby([\"Gender\"]).mean()[\"Price\"]\n",
    "gender_average\n",
    "\n",
    "#Avg Total Purchase Per Person\n",
    "average_per_person = gender_purchase_total/specific_gender_counts_df[\"Total Count\"]\n",
    "average_per_person\n",
    "\n",
    "#Summary Data Frame\n",
    "gender_analysis_df = pd.DataFrame({\"Purchase Count\": purchase_counts,\n",
    "                                    \"Average Purchase Price\": gender_average,\n",
    "                                    \"Total Purchase Value\": gender_purchase_total,\n",
    "                                  \"Avg. Total Purchase per Person\": average_per_person})\n",
    "gender_analysis_df.head()"
   ]
